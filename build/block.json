{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "attributes": {
    "posts": {
      "type": "array",
      "default": []
    },
    "align": {
      "type": "string",
      "default": "none"
    }
  },
  "name": "evermade/posts-plugin",
  "version": "0.1.0",
  "title": "Evermade Posts Plugin",
  "category": "widgets",
  "icon": "grid-view",
  "description": "A block that displays posts in a grid.",
  "supports": {
    "align": [
      "wide"
    ]
  },
  "textdomain": "evermade-posts-plugin",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}